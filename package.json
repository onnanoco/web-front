  "next-unused": {
    "alias": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@contants/*": [
        "src/contants/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@provider/*": [
        "src/provider/*"
      ]
    },
    "include": [
      "src/assets",
      "src/contexts",
      "src/contants",
      "src/components",
      "src/provider"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  }
